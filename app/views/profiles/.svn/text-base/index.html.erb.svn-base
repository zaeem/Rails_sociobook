    <td><%= profile.contact_screenname %></td>
    <td><%= profile.contact_messenger %></td>
    <td><%= profile.contact_phone %></td>
    <td><%= profile.contact_mobile %></td>
    <td><%= profile.contact_countrycode %></td>
    <td><%= profile.contact_zipcode %></td>
    <td><%= profile.contact_address %></td>
    <td><%= profile.contact_town %></td>
    <td><%= profile.contact_neighbour %></td>
    <td><%= profile.contact_website %></td>
    <td><%= profile.tv %></td>
    <td><%= profile.favour_quotes %></td>
    <td><%= profile.political_views %></td>
    <td><%= profile.political_views_desc %></td>
    <td><%= profile.total_friends %></td>
    <td><%= profile.featured_friends %></td>
    <td><%= profile.religion %></td>
    <td><%= profile.religion_name %></td>
    <td><%= profile.religion_logo %></td>
    <td><%= profile.religion_logo_path %></td>
    <td><%= profile.religion_desc %></td>
    <td><%= profile.show_birthdate %></td>
    <td><%= profile.show_gender %></td>
    <td><%= profile.gender %></td>
    <td><%= profile.relation_status %></td>
    <td><%= profile.interested_in %></td>
    <td><%= profile.languages %></td>
    <td><%= profile.aboutme %></td>
    <td><%= profile.birthdate %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

