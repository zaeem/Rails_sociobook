<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.user_id %></td>
    <td><%= profile.yahoo_id %></td>
    <td><%= profile.yahoo_pass %></td>
    <td><%= profile.hotmail_id %></td>
