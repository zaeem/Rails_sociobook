<div class="add_dataset">
  
  <table width="60%">  
    <tr>
      <td><span style="float:right;"/><%= image_tag("relation_logo.png", :size => "50x50")%></td>
      <td width="10%"><%= f.check_box :is_family %></td>
      <td><%= f.label :family_member %></td>
    </tr>
    <tr>
      <td>&nbsp;</td> 
      <td width ="10%"><%= f.label :name %></td>
      <td><%= f.autocomplete_field :family_name, autocomplete_profile_relation_family_name_home_path %></td>
    </tr>
    <tr>
      <td colspan="2"><span style="float:right;"/><%= f.label :family_relation %></td>
      <td><%= f.select :family_relation , [['Select Relation:','None'],['Son','Son'],
            ['Daughter','Daughter'],['Mother','Mother'],['Father','Father'],['Sister','Sister'],
               ['Brother','Brother'],['Uncle','Uncle'],['Niece','Niece'],['Nephow','Nephow']]%></td>
    </tr>
     <tr>
      
      <td colspan="3"><span style="float: right"/>
        <%#*<button type="button" class="delete_div_btn" >Remove</button>%>
                  <%= link_to_remove_fields "Remove", f %>
      </td>

      </tr>
  </table>
</div>
