<h1>Listing cstatuses</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Title</th>
    <th>Text </th>
    <th>Desc</th>
    <th>Misc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cstatuses.each do |cstatus| %>
  <tr>
    <td><%= cstatus.content_id %></td>
    <td><%= cstatus.title %></td>
    <td><%= cstatus.text_ %></td>
    <td><%= cstatus.desc %></td>
    <td><%= cstatus.misc %></td>
    <td><%= link_to 'Show', cstatus %></td>
    <td><%= link_to 'Edit', edit_cstatus_path(cstatus) %></td>
    <td><%= link_to 'Destroy', cstatus, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cstatus', new_cstatus_path %>
