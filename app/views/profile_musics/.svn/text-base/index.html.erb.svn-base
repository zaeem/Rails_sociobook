<h1>Listing profile_musics</h1>

<table>
  <tr>
    <th>Profile</th>
    <th>Title</th>
    <th>Album</th>
    <th>Artist</th>
    <th>Logo</th>
    <th>Logo path</th>
    <th>Desc</th>
    <th>Is default</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profile_musics.each do |profile_music| %>
  <tr>
    <td><%= profile_music.profile_id %></td>
    <td><%= profile_music.title %></td>
    <td><%= profile_music.album %></td>
    <td><%= profile_music.artist %></td>
    <td><%= profile_music.logo %></td>
    <td><%= profile_music.logo_path %></td>
    <td><%= profile_music.desc %></td>
    <td><%= profile_music.is_default %></td>
    <td><%= link_to 'Show', profile_music %></td>
    <td><%= link_to 'Edit', edit_profile_music_path(profile_music) %></td>
    <td><%= link_to 'Destroy', profile_music, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile music', new_profile_music_path %>
