<h1>Listing cpshares</h1>

<table>
  <tr>
    <th>Cpoll</th>
    <th>Profile</th>
    <th>Option</th>
    <th>Isanswered</th>
    <th>Misc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cpshares.each do |cpshare| %>
  <tr>
    <td><%= cpshare.cpoll_id %></td>
    <td><%= cpshare.profile_id %></td>
    <td><%= cpshare.option %></td>
    <td><%= cpshare.isanswered %></td>
    <td><%= cpshare.misc %></td>
    <td><%= link_to 'Show', cpshare %></td>
    <td><%= link_to 'Edit', edit_cpshare_path(cpshare) %></td>
    <td><%= link_to 'Destroy', cpshare, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cpshare', new_cpshare_path %>
