<h1>Listing cphotos</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Title</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
    <th>Photo updated at</th>
    <th>Desc</th>
    <th>Misc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cphotos.each do |cphoto| %>
  <tr>
    <td><%= cphoto.content_id %></td>
    <td><%= cphoto.title %></td>
    <td><%= cphoto.photo_file_name %></td>
    <td><%= cphoto.photo_content_type %></td>
    <td><%= cphoto.photo_file_size %></td>
    <td><%= cphoto.photo_updated_at %></td>
    <td><%= cphoto.desc %></td>
    <td><%= cphoto.misc %></td>
    <td><%= link_to 'Show', cphoto %></td>
    <td><%= link_to 'Edit', edit_cphoto_path(cphoto) %></td>
    <td><%= link_to 'Destroy', cphoto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cphoto', new_cphoto_path %>
