    padding: 0;
    text-align: center;
    width: 70px;
}
#nav li a {
    color: #fff;
    display: block;
    height: 36px;
    font-size: 12px;
}

/* footer */
#footer {
    clear: both;
    margin-top: 48px;
    padding: 24px 0;
    width: 744px;

}

#footer-nav {
    border-top: 1px solid #ccc;
    padding-top: 24px;
}

/* Welcome message */
p.welcome {
    position: absolute;
    right: 0;
    text-align: right;
    top: 12px;
}

.menus {
    position: inherit;
    text-align: left;
    width:200px;
    top: 40px;
}


/* Site elements */

/* List */
.list {
    margin-right: 6px;
}
.list li {
    border-bottom: 1px solid #eee;
    background: none;
    clear: both;
    display: inline;
    float: left;
    list-style: none;
    margin: 0 0 12px 6px;
    padding: 0 0 6px 0;
    width: 100%;
}
.list li.last {
    border-bottom: none;
}
.list .profile-image {
    clear: left;
    float: left;
    display: inline;
}
.list.full .profile-image {
    float: left;
    /* overflow: hidden; this hides the activity icon when no member thumbnail */
    position: relative;
    width: 102px;
}
.list.small .profile-image {
    position: relative;
    width: 60px;
}
.list.small li {
    display: block;
}
.list p {
    margin-bottom: 0;
    margin-left: 102px;
}

.activity p {
    margin-left: 1em;
}

.list .forum p {
    margin-bottom: 1em;

}
.list.small p {
    font-size: 12px;
    margin-left: 60px;
}
.list.full .profile-image img {
    border: 1px solid #fff;
}
.list .profile-image img.icon {
    position: absolute;
    right: 3px;
    top: 0;
}

.blog p {
    margin-left: 0;
    margin-bottom: 1em;
}
.list li ul {
    margin-bottom: 2em;
}
.list li ul li {
    border: 0;
    margin: 0 0 .5em 0;
    padding-left: 24px;
}

.list li li.reply {
    border-top: 1px solid #eee;
    border-bottom: 0;
    margin-left: 84px;
    margin-top: 12px;
    padding: 12px 0 0;
    width: 398px;
}

/* Grid */
.grid {
    clear: both;
    float: left;
    width: 100%;
}
.grid li {
    background: none;
    display: inline;
    float: left;
    list-style-type: none;
    padding: 0;
}
.col2 .contacts.grid li {
    height: 36px;
    margin: 0 0 6px 6px;
    overflow: hidden;
    width: 36px;
}
.col2 .contacts li.more a {
    margin-right: 12px !important;
}
.grid.medium li {
    background: url(/images/blue-grad.jpg) bottom repeat-x;
    border: 1px solid #ccc;
    height: 48px;
    margin: 0 0 12px 6px;
    width: 154px;
}
.grid.medium li, .grid.medium p {
    font-size: 12px;
}
.grid.medium img {
    float: left;
    /*clip: rect(36px, 36px, 36px, 36px);*/
    margin: 0px 6px 6px 0;
}
.grid.medium a {
    display: block;
    height: 100%;
    margin: 6px;
}


/* Profile */

.profile .toolbox {
    clear: left;
    float: left;
    margin: 0 12px 12px 0;
    width: 240px;
}
.profile img.photo {
    margin-bottom: 12px;
}
.profile .note {
    margin-left: 252px;
}
.profile .toolbox ul {
    width: 238px;
}
.profile ul.tools li {
    background: none;
    font-size: 12px;
    list-style-type: disc;
    margin: 6px 6px 6px 2em;
    padding: 0;
}

/* Messages */

#message-nav {
    background: url(/images/silver-grad.jpg) bottom repeat-x;
    border-bottom: 1px solid #ccc;
    float: left;
    height: 24px;
    margin-top: -24px;
    position: relative;
    width: 100%;
}
#message-nav li {
    background: none;
    float: left;
    font-size: 12px;
    line-height: 24px;
    list-style-type: none;
    margin: 0;
    text-align: center;
    padding: 0;
    width: 78px;
}
#message-nav li a:hover {
    background: url(/images/silver-grad-inv.jpg) bottom repeat-x;
}
#message-nav li.compose {
    background: url(/images/compose-t.png) top no-repeat;
    float: right;
    line-height: 10px;
    margin: 3px 3px 0;
    width: 154px;
}
#message-nav li.compose a, #message-nav li.compose a:hover {
    background: url(/images/compose-b.png) bottom no-repeat;
    color: #fff;
    margin-top: 4px;
    padding-bottom: 4px;
}
#message-nav li a {
    color: #333;
    display: block;
    font-weight: normal;
    height: 100%;
}
.message .tools {
    margin-right: 6px;
    text-align: right;
}

/* Gallery list */
#gallery_list li {
    background: url(/images/blue-grad.jpg) bottom repeat-x;
    border: 1px solid #ccc;
    height: 80px;
    margin: 10px 0 12px 0px;
    width: 300px;
}
#gallery_list li div, #gallery_list li p {
    padding-left: 10px;
}
#gallery_list li p {
    float: left;
    width: 200px;
    line-height: 1.6;
    margin-left: 1em;
}
.gallery_title {
    font-size: 146%;
    font-weight: bold;
}
div.toolbox ul li {
    font-size: 12px;
    list-style: disc inside;
    background: none;
    padding-left: 0px
}

div.photo, div.photo-small {
    padding-top: 10px;
    text-align: center;
}

div.gallery-description {
    font-size: 12px;
    text-align: justify;
}

div.photo-small:hover {
    background-color: #ccccff;
}

.form_row a {
    margin-left: 2px;
    font-size: 80%;
}

