
/* Main layout */

/*body {
    text-align: center;
}*/
#header-wrap {
    /* background: #069; */
    background: #069 url('/images/header_background.png');
    float: left;
    width: 100%;
}
#header, #wrap, #footer {
    margin: 0 auto;
    text-align: left;
    width: 744px;
}
#content {
    background: none repeat scroll 0 0 lightgray;
    float: left;
    padding: 0 0 20px 1px;
    width: 100%;
}
.col1 {
    float: left;
    width: 490px;
}
.col2 {
    float: right;
    width: 240px;
}
.single-col .col1 {
    margin: 0 210px;
    width: 324px;
}

/* columns inside a tabbed panel */
/* they need to be narrower because of the space consumed by the tabs 
   interface */
#tabCol .col1 {
    float: left;
    width: 60%;
    padding-right: 10px;  
}
#tabCol .col2 {
    float: right;
    width: 35%;
    margin-left: 20px;  
}
/* Header */
#header {
    position: relative;
    height: 80px;
}

#header h1 {
    float: left;
    margin-top: 12px;
    margin-bottom: 12px;
    width: 144px;
}

/* Header-nav */
#nav {
    margin: 44px 0 0 0;
    width: 560px;
    float: right;
}
#nav li {
    background: #069 url('/images/header-grad.jpg') bottom repeat-x;
    height: 36px;
    line-height: 36px;
    list-style-type: none;
    text-align: center;
    width: 70px;
}

.content{
    margin-top: 60px;
    text-align: center;
}

.content .header{
    font-size: 20px;
    font-weight: bold;
}

.stepList {
    margin: auto;
    width: 600px;
}

.stepElement{
    background: #069 url(/images/header-grad-inv-It.jpg) bottom repeat-x;
    list-style-type: none;
    text-align: center;
    width: 25%;
}


.stepElement .content {
    color: whitesmoke;
    font: 12px;
    margin: 11px 10px 0;
}


.stepElement .description {
    color: whitesmoke;
    font-size: 10px;
}

.stepSelected{
    background: #069 url(/images/header-grad.jpg) repeat-x;
}

.stepSelected .content {
    font-size: 16px;
}

.stepSelected .description {
    font-size: 12px;
}

.stepContent{
  color: whitesmoke;
}
.contentList {
    margin: 10px 100px 0 0;
    width: 560px;
    float: right;
}

.contentElement{
    list-style-type: none;
    text-align: center;
    width: 25%;
}



/* Header-nav */
#nav {
    margin: 44px 0 0 0;
    width: 560px;
    float: right;
}
#nav li {
    background: #069 url(/images/header-grad.jpg) bottom repeat-x;
    float: left;
    height: 36px;
    line-height: 36px;
    list-style-type: none;
    margin: 0 0 0 0;
    padding: 0;
    text-align: center;
    width: 70px;
}
#nav li a {
    color: #fff;
    display: block;
    height: 36px;
    font-size: 12px;
}
#nav li a:hover {
    background: url(/images/header-grad-inv.jpg) top repeat-x;
}
#nav li.active a, #nav li.active a:hover {
    background: #fff;
    color: #069;
}
#nav li.n-home {
    margin-left: 0;
}

/* Header-nav */
#nav {
    margin: 44px 0 0 0;
    width: 560px;
    float: right;
}
#nav li {
    background: #069 url(/images/header-grad.jpg) bottom repeat-x;
    float: left;
    height: 36px;
